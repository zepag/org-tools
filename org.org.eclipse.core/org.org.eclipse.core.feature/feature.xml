<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.org.eclipse.core.feature"
      label="ORG Core Feature"
      version="1.3.0.qualifier"
      provider-name="ORG"
      plugin="org.org.eclipse.core.branding"
      image="ORG_banner.gif">
   <install-handler/>

   <description url="http://www.org-tools.org/core-feature/index.html">
      The ORG feature consists in:
- a base for Eclipse plugins
   </description>

   <copyright url="http://www.org-tools.org/core-feature/copyright.html">
      Copyright ORG 2007. See http://www.org-tools.org/core-feature/copyright.html.
   </copyright>

   <license url="http://www.org-tools.org/core-feature/license.html">
      Licensed under the EPL.
See http://www.org-tools.org/eclipse-dws/license.html.
   </license>

   <url>
      <update label="ORG Tools Stable Releases" url="http://www.org-tools.org/updatesites/stable/3.3/"/>
   </url>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.jdt.core"/>
      <import plugin="org.eclipse.jdt.launching"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.ui.forms"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.core.net"/>
   </requires>

   <plugin
         id="org.org.eclipse.core.ui"
         download-size="0"
         install-size="0"
         version="1.3.0.qualifier"
         unpack="false"/>

   <plugin
         id="org.org.eclipse.core.help"
         download-size="0"
         install-size="0"
         version="1.3.0.qualifier"/>

   <plugin
         id="org.org.eclipse.core.utils.jdt"
         download-size="0"
         install-size="0"
         version="1.3.0.qualifier"
         unpack="false"/>

   <plugin
         id="org.org.eclipse.core.utils.platform"
         download-size="0"
         install-size="0"
         version="1.3.0.qualifier"
         unpack="false"/>

   <plugin
         id="org.org.eclipse.core.branding"
         download-size="0"
         install-size="0"
         version="1.3.0.qualifier"
         unpack="false"/>

</feature>
