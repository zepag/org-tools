<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.org.eclipse.dws"
      label="ORG DWS Feature"
      version="1.3.0.qualifier"
      provider-name="ORG"
      plugin="org.org.eclipse.dws.branding"
      image="dws_banner.gif">
   <install-handler/>

   <description url="http://www.org-tools.org/eclipse-dws/index.html">
      The ORG DWS feature consists in:
- a base for repository exploration plugins
- a Maven 2.0 repository exploration plugin
   </description>

   <copyright url="http://www.org-tools.org/eclipse-dws/copyright.html">
      Copyright ORG 2007. See http://www.org-tools.org/eclipse-dws/copyright.html.
   </copyright>

   <license url="http://www.org-tools.org/eclipse-dws/license.html">
      Licensed under the EPL.
See http://www.org-tools.org/eclipse-dws/license.html.
   </license>

   <url>
      <update label="ORG Tools Stable Releases" url="http://www.org-tools.org/updatesites/stable/3.3/"/>
   </url>

   <requires>
      <import plugin="org.eclipse.jdt.core"/>
      <import plugin="org.org.eclipse.core.utils.platform"/>
      <import plugin="org.org.eclipse.core.ui"/>
      <import plugin="org.eclipse.search"/>
      <import plugin="org.org.eclipse.core.utils.jdt"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.jdt.ui"/>
      <import plugin="org.eclipse.ui.views.properties.tabbed"/>
      <import plugin="org.org.bundles.maven2"/>
      <import plugin="org.org.bundles.repository.crawler"/>
      <import plugin="org.eclipse.core.expressions"/>
   </requires>

   <plugin
         id="org.org.eclipse.dws.branding"
         download-size="0"
         install-size="0"
         version="1.3.0.qualifier"
         unpack="false"/>

   <plugin
         id="org.org.eclipse.dws.help"
         download-size="0"
         install-size="0"
         version="1.3.0.qualifier"/>

   <plugin
         id="org.org.eclipse.dws.core"
         download-size="0"
         install-size="0"
         version="1.3.0.qualifier"
         unpack="false"/>

   <plugin
         id="org.org.eclipse.dws.ui"
         download-size="0"
         install-size="0"
         version="1.3.0.qualifier"
         unpack="false"/>

</feature>
